#!/Users/danielwhite/Documents/Development/GitHub/BazzellCLI/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'BazzellCLI','console_scripts','bazzellCLI'
__requires__ = 'BazzellCLI'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('BazzellCLI', 'console_scripts', 'bazzellCLI')()
    )
